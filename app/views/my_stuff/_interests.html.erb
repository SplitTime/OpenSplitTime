<%# locals: (presenter:) %>

<turbo-frame id="my_stuff_interests_card">
  <% if presenter.interests.any? %>
    <% presenter.interests.each do |person| %>
      <h4 class="card-text">
        <strong><%= link_to person.full_name, person_path(person) %></strong>
      </h4>
    <% end %>
  <% else %>
    <div class="card-text h5">None found</div>
  <% end %>
</turbo-frame>
